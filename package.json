{
  "name": "bunda",
  "version": "0.1.0",
  "description": "Bundles functions for use in AWS Lambda or other cloud code providers",
  "author": "Jason Maurer",
  "license": "MIT",
  "homepage": "https://github.com/jsonmaur/bunda#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonmaur/bunda.git"
  },
  "engines": {},
  "keywords": [],
  "files": [],
  "main": "",
  "bin": {
    "bunda": "bin/bunda"
  },
  "scripts": {
    "test:lint": "standard --verbose | snazzy",
    "test:unit": "ava src/**/*.test.js",
    "test": "npm-run-all test:*",
    "build": "rm -rf lib && babel src -d lib --ignore **/*.test.js",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "npm-run-all": "2.3.0",
    "snazzy": "4.0.0",
    "standard": "7.1.2"
  },
  "dependencies": {
    "archiver": "1.0.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "envify": "3.4.1",
    "minimist": "1.2.0",
    "uglifyify": "3.0.2",
    "zoo": "0.1.8"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
