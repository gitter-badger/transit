{
  "name": "bunda",
  "version": "0.1.0",
  "description": "A serverless web framework for Node and AWS Lambda",
  "author": "Jason Maurer",
  "license": "MIT",
  "homepage": "https://github.com/jsonmaur/bunda#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonmaur/bunda.git"
  },
  "engines": {},
  "keywords": [],
  "files": [],
  "main": "",
  "bin": {
    "bunda": "bin/bunda"
  },
  "scripts": {
    "test:lint": "standard --verbose | snazzy",
    "test:unit": "nyc --all -- mocha src/**/*.test.js --compilers js:babel-register --reporter dot",
    "test:integration": "zoo NODE_ENV=testing mocha test/**/*.js --compilers js:babel-register --reporter dot",
    "test:coverage": "nyc report -r=lcov",
    "test": "rm -rf coverage .nyc_output && npm-run-all test:*",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "mocha": "mocha --compilers js:babel-register",
    "build": "rm -rf lib && babel src -d lib --ignore '**/*.test.js'",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build -- --compact --minified"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "coveralls": "2.11.11",
    "mocha": "2.5.3",
    "npm-run-all": "2.3.0",
    "nyc": "7.0.0",
    "snazzy": "4.0.0",
    "standard": "7.1.2",
    "zoo": "0.1.8"
  },
  "dependencies": {
    "archiver": "1.0.0",
    "aws-sdk": "2.4.9",
    "babel-runtime": "6.9.2",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "chalk": "1.1.3",
    "checksum": "0.1.1",
    "crypto-extra": "0.3.1",
    "deep-assign": "2.0.0",
    "envify": "3.4.1",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.5",
    "minimist": "1.2.0",
    "uglifyify": "3.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it"
    ]
  },
  "nyc": {
    "require": "babel-register",
    "include": [
      "src"
    ]
  }
}
